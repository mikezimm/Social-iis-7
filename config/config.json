{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "socialiis-7-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/socialiis7/Socialiis7WebPart.js",
          "manifest": "./src/webparts/socialiis7/Socialiis7WebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "twitter": {
      "path": "src/javascripts/twitter/widgets.js",
      "globalName": "twttr"
    }
  },
  "localizedResources": {
    "Socialiis7WebPartStrings": "lib/webparts/socialiis7/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}